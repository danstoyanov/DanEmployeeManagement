@model HomeDetailsViewModel

@{
    Layout = "null";
    ViewBag.Title = "Employee Details";
}

<h3>@Model.PageTitle</h3>

<div>
    <b>Name:</b> @Model.Employee.Name
</div>
<div>
    <b>Email:</b> @Model.Employee.Email
</div>
<div>
    <b>Department:</b> @Model.Employee.Department
</div>

@section Scripts {
    <script src="~/js/CustomScript.js"></script>
}